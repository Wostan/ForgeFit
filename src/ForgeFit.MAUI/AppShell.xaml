<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ForgeFit.MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ForgeFit.MAUI"
    xmlns:views="clr-namespace:ForgeFit.MAUI.Views"
    Title="ForgeFit.MAUI">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    
    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate views:LoginPageView}"
        Route="LoginPageView" />

</Shell>
